{
  "semi": false,
  "singleQuote": true,
  "singleAttributePerLine": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "printWidth": 80,
  "arrowParens": "always",
  "endOfLine": "lf",
  "proseWrap": "preserve",
  "plugins": ["prettier-plugin-tailwindcss"]
}
